<p id="notice"><%= notice %></p>

<h1>Lti Users</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Given</th>
      <th>Family</th>
      <th>Username</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lti_users.each do |lti_user| %>
      <tr>
        <td><%= lti_user.full_name %></td>
        <td><%= lti_user.given %></td>
        <td><%= lti_user.family %></td>
        <td><%= lti_user.username %></td>
        <td><%= lti_user.email %></td>
        <td><%= link_to 'Show', lti_user %></td>
        <td><%= link_to 'Edit', edit_lti_user_path(lti_user) %></td>
        <td><%= link_to 'Destroy', lti_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lti User', new_lti_user_path %>
